{
  "name": "npm-workspaces-symlink-bug",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "npm-workspaces-symlink-bug",
      "version": "1.0.0",
      "license": "ISC",
      "workspaces": [
        "packages/*"
      ]
    },
    "node_modules/bar": {
      "resolved": "packages/bar",
      "link": true
    },
    "node_modules/foo": {
      "resolved": "packages/foo",
      "link": true
    },
    "packages/bar": {
      "version": "1.0.0",
      "license": "ISC",
      "dependencies": {
        "foo": "file:packages/foo"
      }
    },
    "packages/bar/node_modules/foo": {
      "resolved": "packages/bar/packages/foo",
      "link": true
    },
    "packages/bar/packages/foo": {},
    "packages/foo": {
      "version": "1.0.0",
      "license": "ISC"
    }
  },
  "dependencies": {
    "bar": {
      "version": "file:packages/bar",
      "requires": {
        "foo": "file:packages/foo"
      },
      "dependencies": {
        "foo": {
          "version": "file:packages/bar/packages/foo"
        }
      }
    },
    "foo": {
      "version": "file:packages/foo"
    }
  }
}
